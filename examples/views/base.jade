!!! 5
html
	head
		meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
		title= pengiTitle
		meta(name='description', content= pengiDescription)
		meta(name='keywords', content= pengiKeywords)
		meta(name='robots', content='index, follow')
		link(rel='shortcut icon', href='/_img/site/favicon.png')
		- for cssFile in pengiCSS
			link(rel='stylesheet', href=cssFile)
		- for jsFile in pengiJS
			script(type='text/javascript', src=jsFile)
		block pengiJS
		- for datum in pengiDumpJS
			script!=
				!{datum.name} = !{JSON.stringify(datum.data)}
	body
		block baseBody
